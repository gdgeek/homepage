{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./js",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost"]
  },
  "include": ["src/**/*"]
}
